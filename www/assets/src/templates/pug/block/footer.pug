-var menuItems = {'Главная':'/index.html', 'О нас':'/about.html', 'Меню':'/menu.html', 'Фотогалерея':'/galery.html', 'Отзывы':'/testimonials.html', 'Контакты':'/contacts.html'}
-var socialsItems = {'fa-vk':'/#', 'fa-instagram':'/#', 'fa-youtube':'/#', 'fa-facebook-official':'/#'}
footer.page-footer
    .container
        .row
            .col.l6.s12
                h5 5 вечеров
                p
                    | ☀ Кафе с домашней кухней и демократичными ценами
                    | ☀ Русская, Европейская, Японская, Итальянская кухни
                    | ☀ Детское меню
                    | ☀ Зал вместимостью до 70 гостей.
                    | ☀ Уютная, комфортная атмосфера
                    | ☀ Пять разных зон для отдыха на любой вкус
                    | ☀ Бар
                    | ☀ Банкеты, Свадьбы, Дни рождения, Торжества
                    | ☀ Приятная музыка
                    | ☀ Парковка
            .col.l3.s12.menu-footer
                h5 Меню
                ul
                    each href,name in menuItems
                        li
                            a(href="/" + href)=name
            .col.l3.s12.social-footer
                h5 Мы в социальных сетях
                ul
                    each href,icon in socialsItems
                        li
                            a(href="" + href)
                                i(class="fa "+ icon, aria-hidden="true")
    .footer-copyright
        .container
            span Создание сайта
            a(href='#') idev.studio
